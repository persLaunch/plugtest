function getCookie(cname) { var name = cname + "="; var decodedCookie = decodeURIComponent(document.cookie); var ca = decodedCookie.split(';'); for(var i = 0; i <ca.length; i++) { var c = ca[i]; while (c.charAt(0) == ' ') { c = c.substring(1); } if (c.indexOf(name) == 0) {  return c.substring(name.length, c.length); } }  return ""; } 

window.dataLayer = window.dataLayer || []; 

function transactionComplete(swl_order, 
                      swl_total_price,
                      swl_shipping_price, 
                      swl_tax_price, 
                      swl_line_items, 
                      swl_first_time_accessed) { 
    var totalDiscount = 0; 
    var currencyCode = 'EUR'; 
   

    console.log('order', swl_order);
    console.log('total_price', swl_total_price);
    console.log('shipping_price', swl_shipping_price);
    console.log('tax_price', swl_tax_price);
    console.log('line_items', swl_line_items);
    console.log('first_time_accessed', swl_first_time_accessed);
}; 

console.log('window', window);

console.log('order', window.swl_order);
console.log('total_price', window.swl_total_price);
console.log('shipping_price', window.swl_shipping_price);
console.log('tax_price', window.swl_tax_price);
console.log('line_items', window.swl_line_items);
console.log('first_time_accessed', window.swl_first_time_accessed);

